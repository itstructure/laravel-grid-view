{
    "name": "itstructure/laravel-grid-view",
    "description": "Grid view for laravel framework",
    "type": "library",
    "keywords": ["laravel", "grid", "gridview", "view", "table"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrey Girnik",
            "email": "girnikandrey@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Itstructure\\GridView\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itstructure\\GridView\\GridViewServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
